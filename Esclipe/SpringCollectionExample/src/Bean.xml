<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="javaCollection" class="com.tutorial.JavaCollection">
		<property name="addrestList">
			<list>
				<value>NDIVIA</value>
				<value>INTEL</value>
				<value>QUALCOMM</value>
				<value>RADEON</value>
			</list>
		</property>

		<property name="addrestSet">
			<list>
				<value>NDIVIA</value>
				<value>INTEL</value>
				<value>QUALCOMM</value>
				<value>RADEON</value>
			</list>
		</property>

		<property name="addreMap">
			<map>
				<entry key="1" value="INDIA" />
				<entry key="2" value="Pakistan" />
				<entry key="3" value="USA" />
				<entry key="4" value="USA" />
			</map>
		</property>
		
		<property name="addressPop">
			<props>
            <prop key = "one">NDIVIA</prop>
            <prop key = "one">INTEL</prop>
            <prop key = "two">QUALCOMM</prop>
            <prop key = "three">RADEON</prop>
            <prop key = "four">RADEON</prop>
         </props>
		</property>
	</bean>


</beans>